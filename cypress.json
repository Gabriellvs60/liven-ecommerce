{
  "custom": {
    "name": "Cypress Test"
  },
  "env": {
    "failSilently": false,
    "SNAPSHOT_BASE_DIRECTORY": "./cypress/screenshots/base",
    "SNAPSHOT_DIFF_DIRECTORY": "./cypress/screenshots/diff"
  },
  "testFiles": "**/*.feature",
  "trashAssetsBeforeRuns": true,
  "screenshotsFolder": "./cypress/screenshots/actual",
  "viewportWidth": 1280,
  "viewportHeight": 720,
  "video": false,
  "waitForAnimations": true
}
